Sub FindMissing()

Application.ScreenUpdating = False
    
    Dim var As Variant
    Dim i As Long
    Dim fullCounter As Long
    Dim bln As Boolean
       
    all = "ALL"
    clear = "CLEAR"
     
    fullCounter = Sheets(all).Cells(Rows.Count, 13).End(xlUp).Row
       
    For i = fullCounter To 1 Step -1

          For Each x In Sheets(clear).Range("B1:B" & Sheets(clear).Cells(Rows.Count, 2).End(xlUp).Row)
                
                With x
                    .NumberFormat = "0"
                    .Value = .Value
                 End With
                              
                If Not IsError(Application.Match(Sheets(all).Cells(i, 13).Value, Sheets(clear).Cells(i, 2).Value, 0)) Then
                Sheets(clear).Cells(i, 2).EntireRow.Interior.ColorIndex = 6
                End If
          Next
          
    Next i

MsgBox ("All Clear. Cleared invoices with no matches identified." & vbNewLine & "non-matching records identified.)")

Application.ScreenUpdating = True
End Sub
